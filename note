init

package main
import "fmt"

variable

var f int


input function

fmt.Scanln(&f)

print function

fmt.Println("Hello world")

if/else


switch

switch{
    case f<0:
      fmt.Println("Wrong Input")
    case f<20:
      fmt.Println("Infrasound")
    case f<20000:
      fmt.Println("Audible")
    default:
      fmt.Println("Ultrasound")
}

for loop

for i:=0; i<5; i++ {
    fmt.Println(i)
  }
